{
  "name": "z-physics",
  "version": "0.0.1",
  "author": "Alexander Alexandrov",
  "description": "Simple mechanics engine",
  "keywords": [
    "physics",
    "mechanics",
    "engine"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/redneckz-students-practice/z"
  },
  "bugs": {
    "url": "https://github.com/redneckz-students-practice/z/issues"
  },
  "homepage": "https://github.com/redneckz-students-practice/z",
  "esnext": "src/index.js",
  "main": "lib/index.js",
  "scripts": {
    "prebuild": "npm run test",
    "build": "cross-env NODE_ENV=production babel src --out-dir lib",
    "pretest": "npm run eslint",
    "test": "cross-env NODE_ENV=test nyc mocha --require babel-core/register src/**/*.spec.js",
    "eslint": "eslint src"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/**/*.spec.js"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "dependencies": {
    "z-math": "file:../z-math"
  },
  "devDependencies": {
    "babel-cli": "~6.26.0",
    "babel-core": "~6.26.0",
    "babel-eslint": "~8.2.1",
    "babel-plugin-tailcall-optimization": "~1.0.12",
    "babel-plugin-transform-class-properties": "~6.24.1",
    "babel-plugin-transform-decorators-legacy": "~1.3.4",
    "babel-plugin-transform-function-bind": "~6.22.0",
    "babel-plugin-transform-object-rest-spread": "~6.26.0",
    "babel-plugin-transform-runtime": "~6.23.0",
    "babel-preset-env": "~1.6.1",
    "chai": "~4.1.2",
    "cross-env": "~5.1.1",
    "eslint": "~4.15.0",
    "eslint-config-airbnb-base": "~12.1.0",
    "eslint-plugin-import": "~2.8.0",
    "mocha": "~4.1.0",
    "nyc": "~11.4.1",
    "sinon": "~4.1.3"
  }
}
